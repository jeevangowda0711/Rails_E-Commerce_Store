<%# app/views/products/index.html.erb %>
<%= link_to "Login", new_session_path unless authenticated? %>

<h1><%= t ".title" %> </h1>

<%= link_to "New Product", new_store_product_path %>

<div id="products">
    <% @products.each do |product|%>
    <div>
        <%= link_to product.name, store_product_path(product) %>
        <%# product.description%>
    </div>
    <% end %>
</div>